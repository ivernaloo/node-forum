    "resolve-glob": "^0.1.8",
    "vinyl": "^1.1.1"
  },
  "devDependencies": {
    "gulp": "^3.9.1",
    "gulp-eslint": "^2.0.0",
    "gulp-format-md": "^0.1.9",
