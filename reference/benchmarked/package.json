    "base": "^0.8.1",
    "base-cwd": "^0.1.6",
    "base-option": "^0.7.0",
    "benchmark": "^2.1.0",
    "define-property": "^0.2.5",
    "file-reader": "^1.1.1",
    "for-own": "^0.1.4",
    "has-values": "^0.1.4",
    "inflection": "^1.10.0",
    "is-glob": "^2.0.1",
    "kind-of": "^3.0.2",
    "lazy-cache": "^1.0.4",
    "log-utils": "^0.1.2",
