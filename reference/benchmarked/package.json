  "version": "0.2.5",
  "homepage": "https://github.com/jonschlinkert/benchmarked",
  "author": "Jon Schlinkert (https://github.com/jonschlinkert)",
  "repository": "jonschlinkert/benchmarked",
  "bugs": {
    "url": "https://github.com/jonschlinkert/benchmarked/issues"
  },
  "license": "MIT",
